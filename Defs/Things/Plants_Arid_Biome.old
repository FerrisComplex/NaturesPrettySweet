   <ThingDef ParentName="PlantBase">
    <defName>TKKN_PlantSucculants</defName>
    <label>succulant</label>
    <description>A decorative plant with leaves or petals that are thick and fleshy, to retain water in arid climates.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>14</Beauty>
    </statBases>
    <graphicData>
      <texPath>TKKN_NPS/Things/Plant/CactusSucculants</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
    <hideAtSnowDepth>0.1</hideAtSnowDepth>
    <ingestible>
      <nutrition>0.05</nutrition>
    </ingestible>
    <plant>
		<fertilityMin>0.03</fertilityMin>
